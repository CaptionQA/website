    <script>
      // Main tab switching (Overall vs Per Domain)
      function showMainTab(tab) {
        // Update main tab buttons
        document.querySelectorAll('.btn-f').forEach(btn => {
          btn.classList.remove('active');
        });
        document.getElementById(tab + '-tab-btn').classList.add('active');

        if (tab === 'overall') {
          // Show overall board, hide domain selector and per-domain boards
          document.getElementById('overall-board').style.display = 'block';
          document.getElementById('domain-selector').style.display = 'none';
          document.getElementById('natural-board').style.display = 'none';
          document.getElementById('document-board').style.display = 'none';
          document.getElementById('ecommerce-board').style.display = 'none';
          document.getElementById('embodiedai-board').style.display = 'none';
        } else if (tab === 'perdomain') {
          // Hide overall board, show domain selector
          document.getElementById('overall-board').style.display = 'none';
          document.getElementById('domain-selector').style.display = 'flex';
          // Show first domain by default
          showDomain('natural');
        }
      }

      // Domain switching (within Per Domain tab)
      function showDomain(domain) {
        // Hide all domain boards
        document.getElementById('natural-board').style.display = 'none';
        document.getElementById('document-board').style.display = 'none';
        document.getElementById('ecommerce-board').style.display = 'none';
        document.getElementById('embodiedai-board').style.display = 'none';

        // Update domain buttons
        document.querySelectorAll('.btn-secondary').forEach(btn => {
          btn.classList.remove('active');
        });

        // Show selected domain
        document.getElementById(domain + '-board').style.display = 'block';
        document.getElementById(domain + '-btn').classList.add('active');
      }

      document.addEventListener('DOMContentLoaded', function() {
        showMainTab('overall');
      });

      // Table sorting
      function sortTable(tableId, columnIndex) {
        const table = document.getElementById(tableId);
        const tbody = table.querySelector('tbody');
        const rows = Array.from(tbody.querySelectorAll('tr'));
        const header = table.querySelectorAll('th')[columnIndex];

        const isAscending = header.classList.contains('asc');

        table.querySelectorAll('th').forEach(th => {
          th.classList.remove('asc', 'desc');
        });

        if (isAscending) {
          header.classList.add('desc');
        } else {
          header.classList.add('asc');
        }

        rows.sort((a, b) => {
          const aValue = a.cells[columnIndex].textContent.trim();
          const bValue = b.cells[columnIndex].textContent.trim();

          const aNum = parseFloat(aValue);
          const bNum = parseFloat(bValue);

          if (!isNaN(aNum) && !isNaN(bNum)) {
            return isAscending ? bNum - aNum : aNum - bNum;
          } else {
            return isAscending ? bValue.localeCompare(aValue) : aValue.localeCompare(bValue);
          }
        });

        rows.forEach(row => tbody.appendChild(row));

        rows.forEach((row, index) => {
          row.cells[0].textContent = index + 1;
        });
      }
    </script>
